

<h1 class="page-title">
 Album's Tracks
</h1>


<table class="table">

        <thead class="thead-dark">
            <tr>
            <th scope="col">#</th>
            <th scope="col">Title</th>
            <th scope="col"> Listen</th>
            </tr>
        </thead>

{{#each albumTracksList}}

        <tbody class="table-body">
            <tr>
            <th scope="row">{{track_number}}</th>
            <td class="tracks">{{name}}</td>
            <td><audio controls src="{{preview_url}}">
                </audio></td>
            </tr>
        </tbody>
{{/each}}

</table>


